 pipeline{
	
	agent any  
	stages {
		stage('Build'){ 
			steps{
			  echo "----Building Project-------"
			}
		}
		stage('Deploy'){
			steps{
			echo "----Deploying Project-------"
			}
		}
		stage('test'){ 
			steps{
			echo "----Testing Project-------"
			}
		}
		stage('release'){
			steps{
			echo "----Releasing Project-------"
			}
		}
	}

 }
